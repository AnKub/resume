<div>
  {{#with main}}
  <main>
    <section>
      {{>title big=false data='My educations' }}

      <ul>
        {{#each educations}}
        <li>
            {{name}}
            {{#unless isEnd}} (in process...) {{/unless}}

        </li>
        {{/each}}
      </ul>
    
    </section>
    <section>
      {{>title data='My certificates'}}

      <ul>
        {{#each certificates}}
        <li>
            {{name}} ({{Id}})
       </li>
        {{/each}}
      </ul>
    
    </section>
</main>
  {{/with}}
</div>
